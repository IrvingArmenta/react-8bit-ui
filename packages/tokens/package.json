    "maketypes": "^1.1.2",
    "microbundle": "^0.14.2",
    "nodemon": "^2.0.15",
    "optimist": "^0.6.1",
    "parcel": "^2.0.1",
    "prettier": "^2.5.1",
    "quicktype": "^15.0.260",
    "theo": "^8.1.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
