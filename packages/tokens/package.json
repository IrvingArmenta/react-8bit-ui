{
  "name": "@irubingu/react-8bit-tokens",
  "version": "1.2.6-alpha.0",
  "description": "generating framework agnostic design tokens with theo",
  "author": "Irving Armenta",
  "license": "MIT",
  "private": true,
  "source": "index.ts",
  "exports": {
    "default": "./build/tokens.ts"
  },
  "scripts": {
    "build": "ts-node src/index.ts",
    "prebuild": "rimraf ./build",
    "postbuild": "quicktype build/tokens.json -o build/React8BitTokensTypes.ts --just-types && prettier **/* --write --ignore-unknown --loglevel=error",
    "dev": "watch-run -p 'src/**/*.ts,src/**/*.tsx' yarn build"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@types/theo": "^8.1.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "json-parse-cli": "^0.4.0",
    "json-to-js": "^1.0.2",
    "maketypes": "^1.1.2",
    "microbundle": "^0.14.2",
    "optimist": "^0.6.1",
    "parcel": "^2.0.1",
    "prettier": "^2.5.1",
    "quicktype": "^15.0.260",
    "rimraf": "^3.0.2",
    "theo": "^8.1.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "watch-run": "^1.2.5"
  }
}
